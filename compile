#!/usr/bin/python
import argparse
import json
import re
import io
import sys

def replace_fields(text, fields):
    """ Replaces custom fields in text with values specified in fields.

    Custom fields have the format '{field_name}'. 
    
    :param text:   The string to be replaced.
    :param fields: A dict with field names as the keys, where each entry is a dict
                   that contains the key 'value' that maps to the replacements.
                   E.g. {'field_name' : {'value' : 'replacement'}, ...}
    :returns: the same content as text, with fields replaced.
    """

    # pattern is something like {(field_name1|field_name2|...)},
    # field name is being captured, and can be retrieved by matchobj.group(1)
    return re.sub(pattern=r'\{(%s)\}' % '|'.join(fields.keys()),
                  repl=lambda match: str(fields[match.group(1)]['value']),
                  string=text) if fields else text

def output_content(html, output, css, js, fields):
    fields = json.load(fields)
    output.write('''<html>
    <head>
        <link href="https://fonts.googleapis.com/css?family=Open+Sans" rel="stylesheet" type="text/css">
        <link href="https://cdnjs.cloudflare.com/ajax/libs/animate.css/3.5.2/animate.min.css" rel="stylesheet">
        <script src="https://cdn.polyfill.io/v2/polyfill.min.js?unknown=polyfill&features=es6|gated|always"></script>
        <script src="https://ajax.googleapis.com/ajax/libs/webfont/1.5.3/webfont.js"></script>
        <script src="https://code.jquery.com/jquery-3.3.1.min.js" integrity="sha256-FgpCb/KJQlLNfOu91ta32o/NMZxltwRo8QtmkMRdAu8=" crossorigin="anonymous"></script>
        <script>
            $.fn.extend({{
                animateCss: function (animationName) {{
                    var animationEnd = 'webkitAnimationEnd mozAnimationEnd MSAnimationEnd oanimationend animationend';
                    this.addClass('animated ' + animationName).one(animationEnd, function() {{
                        $(this).removeClass('animated ' + animationName);
                    }});
                    return this;
                }}
            }});
            
            var loadBoss;
            var damageBoss;

            (function() {{
                var currentHealth = 0;
                var initialHealth = 0;
                var createDate;
                let bossAvatar = 'data:image/png;base64,';

                function getFormattedDateString(d) {{
                    return   [d.getFullYear(), d.getMonth() + 1, d.getDate()].join('-')
                           + ' '
                           + [d.getHours(), d.getMinutes(), d.getSeconds()].join(':');
                }}

                loadBoss = function(health) {{
                    health = +health;
                    document.dispatchEvent(new CustomEvent('bossLoad', {{
                        detail: {{
                            boss_img: bossAvatar,
                            boss_name: 'Carbuncle',
                            created_at: getFormattedDateString(createDate = new Date()),
                            current_health: currentHealth = initialHealth = health,
                            incr_amount: null,
                            mode: 'fixed',
                            multiplier: 1,
                            streamboss_id: 0,
                            total_health: health,
                            updated_at: getFormattedDateString(new Date()),
                            user_id: 0,
                            settings: {{
                                custom_json: {json}
                            }}
                        }}
                    }}));
                }};

                damageBoss = function(health) {{
                    health = +health;
                    if (health >= currentHealth) {{
                        document.dispatchEvent(new CustomEvent('bossKilled', {{
                            detail: {{
                                boss: {{
                                    boss_img: bossAvatar,
                                    boss_name: 'Carbuncle',
                                    created_at: getFormattedDateString(createDate = new Date()),
                                    current_health: '' + initialHealth,
                                    incr_amount: null,
                                    mode: 'fixed',
                                    multiplier: 1,
                                    streamboss_id: 0,
                                    total_health: health,
                                    updated_at: getFormattedDateString(new Date()),
                                    user_id: 0
                                }},
                                message: {{
                                    damage: '- ' + currentHealth,
                                    text: 'Carbuncle killed!!',
                                }}
                            }}
                        }}));
                    }} else {{
                        document.dispatchEvent(new CustomEvent('bossDamaged', {{
                            detail: {{
                                boss: {{
                                    boss_img: bossAvatar,
                                    boss_name: 'Carbuncle',
                                    created_at: getFormattedDateString(createDate),
                                    current_health: currentHealth -= health,
                                    incr_amount: null,
                                    mode: 'fixed',
                                    multiplier: 1,
                                    streamboss_id: 0,
                                    total_health: health,
                                    updated_at: getFormattedDateString(new Date()),
                                    user_id: 0
                                }},
                                message: {{
                                    damage: '- ' + health,
                                    text: 'Carbuncle attacked!!',
                                    type: 'dmg'
                                }}
                            }}
                        }})); 
                    }}
                }};
            }})();

        </script>
        <script>{js}</script>
        <style>{css}</style>
    </head>
    <body style="background-color: black; color: white">
        <div id="sl__streamboss" style="margin:0px">
            <div id="wrap">{html}</div>
        </div>
        <form id="controls" action="#">
            <fieldset>
                <label for="health">Health</label>
                <input type="number" id="health" name="health" value="1000">
            </fieldset>

            <fieldset>
                <label for="load_boss">Load boss</label>
                <input type="button" id="load_boss" name="load_boss" value="Load Boss with Initial Health" onclick="loadBoss($('#health', this.form).val())">
            </fieldset>

            <fieldset>
                <label for="damage_boss">Damage boss</label>
                <input type="button" id="damage_boss" name="damage_boss" value="Damage Boss with Health" onclick="damageBoss($('#health', this.form).val())">
            </fieldset>
        </form>
    </body>
</html>'''.format(js=js.read(), 
                  css=replace_fields(css.read(), fields), 
                  html=replace_fields(html.read(), fields),
                  json=json.dumps(fields)))
    
def parse_args(): 
    parser = argparse.ArgumentParser(
                description='''
                            Generates a demo html file for the Streamboss widget, with CSS, JavaScript
                            and custom fields plugged in.
                            '''
             )
    parser.add_argument('html', metavar='html.html', default=sys.stdin, type=argparse.FileType('r'), nargs='?',
                        help='''
                             The template html file. Contains the exact html content you would copy to
                             the Streamboss widget as the html. Custom fields are replaced. Reads from
                             standard input if not specified.
                             ''')
    parser.add_argument('-c', '--css', metavar='styles.css', default=io.StringIO(), type=argparse.FileType('r'), nargs='?', required=False,
                        help='''
                             The template css file. Contains the exact css content you would copy to the
                             Streamboss widget as the css. Custom fields are replaced.
                             ''')
    parser.add_argument('-j', '--js', metavar='javascript.js', default=io.StringIO(), type=argparse.FileType('r'), nargs='?', required=False,
                        help='''
                             The JavaScript file. Contains the exact JavaScript code you would copy to
                             the Streamboss widget as the JavaScript content. Custom fields are not
                             replaced.
                             ''')
    parser.add_argument('-f', '--fields', metavar='fields.json', default=io.StringIO(u'{}'), type=argparse.FileType('r'), nargs='?', required=False,
                        help='''
                             The JSON file containing the custom fields. Contains the exact JSON you would
                             copy to the Streamboss widget as the custom fields. Used to replace the
                             fields in the html and css content.
                             ''')
    parser.add_argument('-o', '--output', metavar='output.html', default=sys.stdout, type=argparse.FileType('w'), nargs='?',
                        help='''
                             The output html file. CSS and JavaScript are embeded. Also adds extra buttons
                             and fields on the page for testing purposes. Outputs to standard output if not
                             specified.
                             ''')

    return parser.parse_args()

if __name__ == '__main__':
    output_content(**vars(parse_args()))
